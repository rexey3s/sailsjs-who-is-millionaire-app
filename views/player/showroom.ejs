<div id="header">

      <div id="logo">
      </div>
      <div id="supports">
      <img id="logout-btn" src="/images/icon-logout.png" height="52" width="52" />
      </div>

</div>
    <div class="q-wrapper ">
      <div class="function-btns-wrapper" >
        <div class="function-btn ready-btn">Sẵn sàng</div>
        <a href="/player/disjoin"><div class="function-btn disjoin-btn">Rời phòng</div></a>

      </div>
      <div class="clearfix"></div>
    </div>
    <% if(typeof thisPlayer !== 'undefined') { %>
    <input id="thisPlayer" type="hidden" value="<%= thisPlayer.id %>"/>
    <% } %>

    <div id="player-list-wrapper" data-model="room" data-room="<%=room.name %>"class="" >
    <h2 class=".player-num alert alert-success"><center>Phòng: <%-'<b>'+room.name+'</b>'%> | Số người tham gia: <%-   '<b id="player-num">'+players.length+'</b>' %></center></h2>

        <div id="player-list-holder">
          <div class="player-main-list">
            <ul id="joined-list" class="player-list">
            <%_.each(players,function(player) { %>

             <li data-model="player" data-id="<%= player.id%>" class="player">

                <div class="ava"></div>
                <div class="info" data-model="player" data-id="<%= player.id%>">
                  <div class="name"><%= player.name %></div>
                   <div class="position">Người tham gia</div>
                 <% if(player.isReady)  { %>
                  <div class="status green">Sẵn sàng</div>
                  <% } else { %>
                  <div class="status red">Chưa sẵn sàng</div>

                  <% }%>
                </div>
                <!-- <div class="remove-player"></div> -->
              </li>
              <% });%>

            </ul>
            <div class="clearfix"></div>
          </div>

        </div>
    </div>

